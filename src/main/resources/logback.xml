<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%level] [%date{yyyy-MM-dd'T'HH:mm:ss.SSSZ}] [%thread] [%logger{36}] - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.tshinow" level="DEBUG" />
  <root level="INFO">
    <appender-ref ref="console" />
  </root>
</configuration>
